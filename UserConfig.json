    "cuda_fuse_any": false,
    "vectorized_single_queue": false,
    "debug_llvm": false,
    "explain_cuda_codegen": false,
    "explain_kernels": true,
    "explain_llvm": false,
    "explain_parsing": false,
    "explain_parsing_simplified": false,
    "explain_property_inference": false,
    "explain_sql": false,
